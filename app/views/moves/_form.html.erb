<%= form_for @move do |f| %>
    <%= f.label :name  %>
    <%= f.text_field :name %>
    <%= f.label :category %>
    <%= f.text_field :category %>
    <%= f.label :position %>
    <%= f.text_field :position %>
    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.label :image %>
    <%= f.text_field :image, :required => true %>

    <%= f.submit %>
<% end %>